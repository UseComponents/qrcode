{
  "name": "qrcode.vue-next",
  "type": "module",
  "version": "0.0.10",
  "keywords": [
    "qrcode",
    "vue",
    "vue-qrcode",
    "qrcode.vue",
    "qrcode-vue-next",
    "qrcode.vue3",
    "vue3-qrcode",
    "vue3-qrcode-next"
  ],
  "author": "UseComponents",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UseComponents/qrcode.git"
  },
  "homepage": "https://usecomponents.github.io/qrcode/",
  "exports": {
    ".": {
      "types": "./es/components/index.d.ts",
      "import": "./es/components/index.js",
      "require": "./lib/components/index.js"
    },
    "./es/*": "./es/*",
    "./lib/*": "./lib/*",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "./es/components/index.js",
  "types": "./es/components/es/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "cross-env ROLLDOWN_OPTIONS_VALIDATION=loose vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build:docs": "vite build --config vite.build.config.ts"
  },
  "dependencies": {
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^18.19.112",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.2.6",
    "prettier": "^3.6.1",
    "typescript": "~5.2.2",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.27"
  }
}
